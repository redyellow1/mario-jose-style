          "_customHTML": "<section class=\"progress-bar1\" group=\"Progress Bars\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <header>Progress bars</header>\n        <input type=\"range\" inline=\"\" title=\"Progress count\" name=\"progressCount\" min=\"1\" max=\"6\" step=\"1\" value=\"3\">\n\n        <input type=\"range\" inline=\"\" title=\"Progress 1\" name=\"progress1\" min=\"0\" max=\"100\" step=\"1\" value=\"72\">\n        <input type=\"range\" inline=\"\" title=\"Progress 2\" name=\"progress2\" min=\"0\" max=\"100\" step=\"1\" value=\"84 \" condition=\"progressCount>1\">\n        <input type=\"range\" inline=\"\" title=\"Progress 3\" name=\"progress3\" min=\"0\" max=\"100\" step=\"1\" value=\"65\" condition=\"progressCount>2\">\n        <input type=\"range\" inline=\"\" title=\"Progress 4\" name=\"progress4\" min=\"0\" max=\"100\" step=\"1\" value=\"79\" condition=\"progressCount>3\">\n        <input type=\"range\" inline=\"\" title=\"Progress 5\" name=\"progress5\" min=\"0\" max=\"100\" step=\"1\" value=\"56\" condition=\"progressCount>4\">\n        <input type=\"range\" inline=\"\" title=\"Progress 6\" name=\"progress6\" min=\"0\" max=\"100\" step=\"1\" value=\"43\" condition=\"progressCount>5\">\n\n        <header>Background</header>\n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background3.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n\n        <div class=\"mbr-row mbr-jc-c\" mbr-class=\"{'mbr-px-2': fullWidth}\">\n            <div class=\"mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 imgblock md-pb\">\n                <amp-img src=\"@PROJECT_PATH@/assets/images/mbr-1140x745.jpg\" layout=\"responsive\" width=\"710.0134228187919\" height=\"464\" alt=\"image\" class=\"mobirise-loader raised\">\n                    <div placeholder=\"\" class=\"placeholder\"></div>\n                    <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/mbr-1140x745.jpg\" alt=\"\" title=\"\">\n                </amp-img>\n            </div>\n\n            <div class=\"mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 mbr-m-auto progress-wrap\">\n                <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left': titleAlign == 1, 'align-center': titleAlign == 2, 'align-right': titleAlign == 3}\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style mbr-bolder\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">\n                        OUR TEAM</h2>\n                    <h3 class=\"mbr-section-subtitle mbr-pt-1 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">\n                        Lorem ipsum dolor sit amet, consectetur adipisicing elit.\n                    </h3>\n                </div>\n                <div class=\"progress_elements md-pb\">\n                    <div class=\"progress1 progressbar\" mbr-class=\"{'last-child': progressCount == 1, 'mbr-pb-4': progressCount > 1}\">\n                        <div class=\"title-progressbar-wrap mbr-flex\">\n                            <p class=\"progressbar-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".title-progressbar-wrap .progressbar-title\">\n                                Service Managers</p>\n                            <p class=\"progress_value mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".progress_value\">%</p>\n                        </div>\n                        <div class=\"progressbar-wrap\">\n                            <div class=\"progress\"></div>\n                        </div>\n                    </div>\n\n                    <div class=\"progress2 progressbar\" mbr-if=\"progressCount > 1\" mbr-class=\"{'last-child': progressCount == 2, 'mbr-pb-4': progressCount > 2}\">\n                        <div class=\"title-progressbar-wrap mbr-flex\">\n                            <p class=\"progressbar-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".title-progressbar-wrap .progressbar-title\">\n                                Electricians</p>\n                            <p class=\"progress_value mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".progress_value\">%</p>\n                        </div>\n                        <div class=\"progressbar-wrap\">\n                            <div class=\"progress\"></div>\n                        </div>\n                    </div>\n\n                    <div class=\"progress3 progressbar\" mbr-if=\"progressCount > 2\" mbr-class=\"{'last-child': progressCount == 3, 'mbr-pb-4': progressCount > 3}\">\n                        <div class=\"title-progressbar-wrap mbr-flex\">\n                            <p class=\"progressbar-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".title-progressbar-wrap .progressbar-title\">Service Engineers</p>\n                            <p class=\"progress_value mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".progress_value\">%</p>\n                        </div>\n                        <div class=\"progressbar-wrap\">\n                            <div class=\"progress\"></div>\n                        </div>\n                    </div>\n\n                    <div class=\"progress4 progressbar\" mbr-if=\"progressCount > 3\" mbr-class=\"{'last-child': progressCount == 4, 'mbr-pb-4': progressCount > 4}\">\n                        <div class=\"title-progressbar-wrap mbr-flex\">\n                            <p class=\"progressbar-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".title-progressbar-wrap .progressbar-title\">\n                                Another</p>\n                            <p class=\"progress_value mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".progress_value\">%</p>\n                        </div>\n                        <div class=\"progressbar-wrap\">\n                            <div class=\"progress\"></div>\n                        </div>\n                    </div>\n\n                    <div class=\"progress5 progressbar\" mbr-if=\"progressCount > 4\" mbr-class=\"{'last-child': progressCount == 5, 'mbr-pb-4': progressCount > 5}\">\n                        <div class=\"title-progressbar-wrap mbr-flex\">\n                            <p class=\"progressbar-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".title-progressbar-wrap .progressbar-title\">\n                                Another</p>\n                            <p class=\"progress_value mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".progress_value\">%</p>\n                        </div>\n                        <div class=\"progressbar-wrap\">\n                            <div class=\"progress\"></div>\n                        </div>\n                    </div>\n\n                    <div class=\"progress6 progressbar\" mbr-if=\"progressCount > 5\" mbr-class=\"{'last-child': progressCount == 6}\">\n                        <div class=\"title-progressbar-wrap mbr-flex\">\n                            <p class=\"progressbar-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".title-progressbar-wrap .progressbar-title\">\n                                Another</p>\n                            <p class=\"progress_value mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".progress_value\">%</p>\n                        </div>\n                        <div class=\"progressbar-wrap\">\n                            <div class=\"progress\"></div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
